<div class="modal fade" id="quotationModalCta" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-md modal-dialog-centered" role="document">
      <div class="modal-dialog" role="document">
        <div class="modal-content pt-1 mb-1">
          <div class="modal-header mt-1">
            <h5 class="mt-1 mb-1 text-center">New Quotation</h5>
            </div>
        <div class="form-container">
              <!--form action="#" class="contact-form form-validate" novalidate="novalidate"-->
                    <h6 class="mt-1 mb-3 text-center"> <%= booking.bookingTitle %></h6>
                    <span name="error" id="err_bm"></span>
                    <div class="row mt-1">
                    <div class="col-lg-6 col-md-6 col-12">
                      <div class="form-group">
                          <input type="number" class="form-control" id="budget" name="contactFirstName" placeholder="0.0" >
                      </div>
                    </div>

                    <div class="col-lg-6 col-md-6 col-12">
                      <div class="form-group">
                          <select name="budgetType" id="budgetType" class="form-control"  >
                            <option value="Per hour"> Per hour</option>
                            <option value="Per project"> Per project</option>
                            <option value="Percentage of total budget"> % of total budget</option>
                          </select>
                      </div>
                    </div>
                    
                  
                    <div class="col-lg-12 col-md-12 col-12  ">
                    <div class="form-group">
                    <label class="required-field" for="quotationDesc">Description</label>
                    <textarea class="form-control" id="quotationDesc" name="quotationDesc" rows="4" oninput="this.className = 'form-control'" placeholder="Provide the reason for a different budget"></textarea>
                    </div>
                    </div>
                    <div class="col mb-2">
                    <input type="button" onClick="sendQuotation('<%=booking._id %>')" name="submit" class="btn button-primary btn-lg w-100 hover-lift-light mt-4-ver" value="Send">
                    </div>
                    </div>
                <!--/form-->
              </div>
  
          </div>
        </div>
      </div>
    </div>
    