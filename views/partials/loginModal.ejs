        <!-- Pills navs -->
        <!-- Pills content -->
        <div class="tab-content ">
          <div class="tab-pane fade show active" id="pills-login" role="tabpanel" aria-labelledby="tab-login-u">
            
            <form action="/login-u" method="post">
              <div class="text-center ">
                <h5 class="title text text-center">Sign in as User</h5> 
                <button type="button" class="btn  btn-floating mx-1">
                  <a class="fab fa-facebook-f" href="/auth/facebook"></a>
                </button>
        
                <button type="button" class="btn  btn-floating mx-1">
                  <a class="fab fa-google"  href="/auth/google"></a>
                </button>
        
                <button type="button" class="btn btn-floating mx-1">
                  <a class="fab fa-twitter"></a>
                </button>
        
                <button type="button" class="btn  btn-floating mx-1">
                  <a class="fab fa-github"></a>
                </button>
              </div>
                <p class="big text text-center">Or fill in information below.</p>
              
            
                <div class="row">
                  <div class="col-md-6">
                      <!-- Email input -->
                      <div class="form-outline ">
                          <input type="email" id="loginEmail" class="form-control" placeholder="Email" name="username"  autocomplete="on" required/>
                      </div>
                  </div>
                  <div class="col-md-6">
                      <!-- Password input -->
                     <div class="form-outline">
                         <input type="password" id="loginPassword" class="form-control" placeholder="Password" name="password" autocomplete="on"  required/>
                     </div>
                  </div>
              </div>
        
              <div class="row">
                <div class="col">
                    <div class="mb-3 justify-content-center">
                            <!-- Submit button -->
                            <button type="submit" class="btn button-primary btn-block mb-4" name="login-u">Login</button>
                    </div>
                </div>
               </div>
            </form>
          </div>
          <div class="tab-pane fade" id="pills-login-p" role="tabpanel" aria-labelledby="tab-login-p">
            <form  action="/login-p" method="post">
              <div class="text-center mb-3">
                <h5 class="title text text-center">Sign in as Service Provider</h5> 
                <button type="button" class="btn btn-floating mx-1">
                  <a class="fab fa-facebook-f" href="/auth/facebook"></a>
                </button>
        
                <button type="button" class="btn btn-floating mx-1">
                  <a class="fab fa-google"  href="/auth/google"></a>
                </button>
        
                <button type="button" class="btn btn-floating mx-1">
                  <a class="fab fa-twitter"></a>
                </button>
        
                <button type="button" class="btn  btn-floating mx-1">
                  <a class="fab fa-github"></a>
                </button>
              </div>
              <p class="big text text-center">Or fill in information below.</p>
             
            <div class="row">
                <div class="col-md-6">
                    <!-- Email input -->
                    <div class="form-outline ">
                        <input type="email" id="ploginEmail" class="form-control" placeholder="Email" name="username" autocomplete="on" required/>
                    </div>
                </div>
                <div class="col-md-6">
                    <!-- Password input -->
                   <div class="form-outline">
                       <input type="password" id="ploginPassword" class="form-control" placeholder="Password" name="password" autocomplete="on" required/>
                   </div>
                </div>
            </div>
          
              <div class="row">
                <div class="col">
                    <!-- Submit button -->
                    <button type="submit" class="btn button-primary btn-block mb-4" name="login-p">Log in</button>
