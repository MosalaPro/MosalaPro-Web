                        <div class="form-outline  ">
                            <input type="number" id="registerPhone" class="form-control" placeholder="Phone Number" name="phone" pattern="/((\+*)((0[ -]*)*|(*))((\d{12})+|(\d{10})+))|\d{5}([- ]*)\d{6}/" required/>
                        </div>
                    </div>
                </div>
                <!-- Address input -->
                <div class="row">
                    <div class="col">
                        <div class="form-outline  ">
                            <input type="text" id="registerAddress" class="form-control" placeholder="Address" name="address" />
                        </div>
                    </div>
                </div>
                <div class="row">
