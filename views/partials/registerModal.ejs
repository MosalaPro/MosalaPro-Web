                            <option value="usa">USA</option>
                            <option value="nigeria">Nigeria</option>
                        </select>
                    </div>
                    <div class="col-md-6">
                        <!-- City input -->
                        <select class="form-control" aria-label="Select City" name="registerCity">
                            <option selected>Select City</option>
                            <option value="ghana">City 1</option>
                            <option value="usa">City 2</option>
                            <option value="nigeria">City 3</option>
                        </select>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <!-- First name input -->
                        <div class="form-outline  ">
                            <input type="text" id="registerState" class="form-control" placeholder="State" name="state"/>
                        </div>
                    </div>
                    <div class="col-md-6">
