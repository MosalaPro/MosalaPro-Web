                        <div class="form-outline  ">
                            <input type="password" id="registerConfPassword" class="form-control" placeholder="Confirm Password"  required/>
                        </div>
                    </div>
                </div>
        
                <!-- 2 column grid layout -->
                <div class="row  ">
                    <div class="col-md-6 d-flex justify-content-center">
